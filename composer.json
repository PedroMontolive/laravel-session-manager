{
    "name": "pemto/laravel-session-manager",
    "description": "Session management for laravel 11.0 and up",
    "type": "library",
    "keywords": [
        "laravel",
        "session",
        "manager",
        "pemto"
    ],
    "license": "MIT",
    "homepage": "https://github.com/PedroMontolive/laravel-session-manager",
    "support": {
        "issues": "https://github.com/PedroMontolive/laravel-session-manager/issues",
        "source": "https://github.com/PedroMontolive/laravel-session-manager"
    },
    "authors": [
        {
            "name": "PedroMontolive",
            "email": "pedro.montolive@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/database": "^11.21",
        "illuminate/support": "^11.21",
        "ramsey/uuid": "^4.7",
        "symfony/console": "^7.1",
        "symfony/process": "^7.1",
        "symfony/error-handler": "^7.1",
        "illuminate/contracts": "^11.21"
    },
    "autoload": {
        "psr-4": {
            "Pemto\\SessionManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "development"
        },
        "laravel": {
            "providers": [
                "Pemto\\SessionManager\\SessionManagerServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}